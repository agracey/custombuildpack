#!/bin/bash

echo "Running Supply Script of custom buildpack"
export BUILDPACK_DIR=`dirname $(readlink -f ${BASH_SOURCE%/*})`


echo $0 $1 $2 $3 $4

echo "find BUILDPACK_DIR"
find $BUILDPACK_DIR

echo "find $1"

find $1

find "find $2"
find $2


echo "find $3"

find $3

pwd
whoami

echo "Hello world" > index.html

exit 0